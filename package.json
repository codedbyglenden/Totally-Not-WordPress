{
	"name": "totally-not-wordpress",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"test": "stylelint 'assets/src/scss/**/*.scss' && eslint assets/src/**/*.ts && phpcbf -p -v . --standard=./phpcs.xml.dist --extensions=php --ignore=/vendor,/node_modules,/tribe,/tribe-events $PWD && phpcs -p -s -v -n . --standard=phpcs.xml.dist --extensions=php --ignore=/vendor,/node_modules $PWD",
		"test-ts": "tsc --noemit --noEmitOnError; eslint assets/src/**/*.ts",
		"test-php": "phpcs -p -s -v -n . --standard=phpcs.xml.dist --extensions=php --ignore=/vendor,/node_modules,/tribe,/tribe-events $PWD",
		"test-styles": "stylelint 'assets/src/scss/**/*.scss'",
		"fix-ts": "eslint assets/src/**/*.ts --fix",
		"fix-scss": "stylelint 'assets/src/scss/**/*.scss' --fix",
		"fix-php": "phpcbf -p -v . --standard=./phpcs.xml.dist --extensions=php --ignore=/vendor,/node_modules,/tribe,/tribe-events $PWD",
		"watch-scss": "gulp sass:watch --gulpfile compilers/gulpfile.js",
		"build-scss": "gulp sass --gulpfile compilers/gulpfile.js",
		"watch-ts": "vite build --config compilers/vite.config.ts --watch",
		"build-ts": "vite build --config compilers/vite.config.ts",
		"production": "vite build --config compilers/vite.config.ts && gulp sass --gulpfile compilers/gulpfile.js"
	},
	"devDependencies": {
		"@types/web": "^0.0.79",
		"@typescript-eslint/eslint-plugin": "^5.46.0",
		"@typescript-eslint/parser": "^5.30.5",
		"@vitejs/plugin-react": "^3.0.0",
		"@wordpress/stylelint-config": "^21.4.0",
		"eslint": "^8.19.0",
		"eslint-import-resolver-typescript": "^3.2.5",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.0",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^8.0.0",
		"gulp-dart-sass": "^1.0.2",
		"gulp-rename": "^2.0.0",
		"gulp-uglifycss": "^1.1.0",
		"path": "^0.12.7",
		"stylelint": "^14.15.0",
		"stylelint-config-prettier-scss": "^0.0.1",
		"stylelint-config-recommended-scss": "^8.0.0",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-config-wordpress": "^17.0.0",
		"stylelint-scss": "^4.3.0",
		"typescript": "^4.9.4",
		"vite": "^3.0.1"
	}
}
